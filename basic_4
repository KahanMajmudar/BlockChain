pragma solidity ^0.5.0;


contract SampleContract{
    uint8 [3] nums = [10,20,30];
    
    function getNums() public returns (uint8 [3] memory){           //returns the data type
        
        nums[0] = 11;
        nums[1] = 22;
        nums[2] = 33;
        return nums;            //returns the data
    }
    
    function getLength() view public returns (uint){
        return nums.length;
    }
}
